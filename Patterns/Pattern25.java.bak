import java.util.Scanner;
class Pattern25
{
	public static void main(String[] args) 
	{
		Scanner sc=new Scanner(System.in);
		System.out.println("Enter the value : ");
		int n = sc.nextInt();
		char a='0';
		char c='a';
		for (int i=0;i<n ;i++ )
		{
			for (int j=0;j<n ;j++ )
			{	
				if (j%2==0)
				{
					System.out.print(a++ +" ");
					if (a=='7')
					{
						a=(char)'M';
					}
					else if(a=='Q')
					{
						a+=35;
					}
				}
				else
				{
				System.out.print(c++ +" ");
				}
			}
			System.out.println();
		}
	}
}
