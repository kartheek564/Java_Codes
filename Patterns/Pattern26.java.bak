import java.util.Scanner;
class Pattern26
{
	public static void main(String[] args) 
	{
		Scanner sc=new Scanner(System.in);
		System.out.println("Enter the value : ");
		int n=sc.nextInt();
		int a=1;
		for (int i=0;i<n ;i++ )
		{
			char c='a';
			for (int j=0;j<n ;j++ )
			{
				if (i>=j)
				{
					System.out.print(a++ +" ");
					if (a==10)
					{
						a=1;
					}
				}
				else
				{
					System.out.print(c++ +" ");
				}
			}
			System.out.println();
		}
	}
}
