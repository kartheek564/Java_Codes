import java.util.Scanner;
class Solution {

    public static void main(String[] args) {
    Scanner sc=new Scanner(System.in);
    long i,sum=0;
    
    int sizel=sc.nextInt();
    long n[]=new long[sizel];
    for(i=0;i<sizel;i++)
    {
    n[i]=sc.nextLong();
    }
    for(i=0;i<sizel;i++)
    {
        sum=sum+(int)n[i];
    }
    System.out.println(sum);
    }
}
