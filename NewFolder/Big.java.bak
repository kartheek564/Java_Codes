import java.util.Scanner;
class  Big
{
	public static void main(String[] args) 
	{
		Scanner sc=new Scanner(System.in);
		System.out.print("Enter a number : ");
		int a=sc.nextInt();
		if(a%2==1)
		{
			System.out.println(a+" is an odd number");
		}
		else
		{
			System.out.println(a+" is an even number");
		}
	}
}
