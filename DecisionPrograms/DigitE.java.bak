class  DigitE
{
	public static void main(String[] args) 
	{
		int a='6';
		if(a>='0'&&a<='9')
		{
			System.out.println('a'+ " is Digit ");
		if((a%2==0))
		{
			System.out.println('a'+ " it is even number");
		}
		else
		{
			System.out.println('a'+ " it is not even number");
		}
		}
		else
		{
			System.out.println(a);
		}
		
	}
}
