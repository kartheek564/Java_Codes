interface Car 
{
	int var=10;
	public void createCar();
}
abstract class Bike implements Car
{
	public void createCar(int var)
	{
		System.out.println("Bike value is :"+var);
	}
}
class MainClass
{
	public static void main(String[] args) 
	{
		Bike bike = new Bike();
		bike.createCar(200);
		System.out.println("Car value is :"+Car.var);
	}
}